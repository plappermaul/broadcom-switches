# MV2 port config: 10x100G
#
# F.HG42 can be converted to other possible configs by changing
# portmap and pbmp_xport_xe config variables.

portmap_1=25:100
portmap_5=29:100
portmap_6=33:100

port_flex_enable=1

# Management Port
portmap_64=81:10:4
# Loopback Port
portmap_65=82:10

core_clock_frequency=1525
dpp_clock_ratio=2:3

oversubscribe_mode=1
pbmp_xport_xe=0x111fffffe11fffffe
l2_mem_entries=32768
l3_mem_entries=16384
fpem_mem_entries=16384
l2xmsg_mode=1

#Software config lane swaps and polarity flips

phy_chain_tx_polarity_flip_physical{25}=0x0

phy_chain_tx_polarity_flip_physical{26}=0x1

phy_chain_tx_polarity_flip_physical{27}=0x1

phy_chain_tx_polarity_flip_physical{28}=0x1

 
