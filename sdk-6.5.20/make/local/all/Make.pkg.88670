FEATURE_LIST := CINT L3 I2C MEM_SCAN EDITLINE BCM_SAL_PROFILE CUSTOMER TEST CHASSIS MSTP RCPU ATPTRANS_SOCKET DUNE_UI INTR APIMODE PTP KBP


BCM_PTL_SPT=1

BCM_88650_A0=1
BCM_88660_A0=1
BCM_88675_A0=1
BCM_88375_A0=1
BCM_88375_B0=1
BCM_88650_B0=1
