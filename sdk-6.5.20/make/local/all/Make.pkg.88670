KERN_VER=3.7.10
#Build a 64 bit executable
WRX_64BIT=1
CFGFLAGS += -D__DUNE_LINUX_BCM_CPU_PCIE__ -D_SIMPLE_MEMORY_ALLOCATION_=0

DEBUG_CFLAGS=-Wdeclaration-after-statement

CFGFLAGS += -DBCM_WARM_BOOT_SUPPORT
