#echo "tr 102: Egr VLAN xlate Ovrflw Ins Test ..."
#tr 102
#$done
#date

echo "tr 103: Egr VLAN xlate Hashing Test ..."
tr 103
$done
date

#echo "tr 104: MPLS Overflow Ins Test ..."
#tr 104
#$done
#date

echo "tr 105: MPLS Hashing Test ..."
tr 105
$done
