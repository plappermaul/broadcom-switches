echo "tr 90: TX Reload Test ..."
#tr 90
#$done
#date

#echo "tr 91: RX Reload Test ..."
#tr 91
#$done
#date

#echo "tr 100: VLAN xlate Overflow Ins Test ..."
#tr 100
#$done
#date

echo "tr 101: VLAN xlate Hashing Test ..."
tr 101
$done
