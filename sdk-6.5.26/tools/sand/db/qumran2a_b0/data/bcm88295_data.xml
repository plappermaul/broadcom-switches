                    <key name="speed" size="dnx_stat_stif_mgmt_port_nof_speeds"></key>
                    <value name="valid" default="0"></value>
                    <value name="ports_type" default="NOT_SUPPORTED"></value>
                    <entries>
                        <entry mode="dnx_stat_stif_mgmt_port_mode_single"  speed="dnx_stat_stif_mgmt_port_speed_100000"  valid="1"  ports_type="CGE"></entry>
                    </entries>
                </table>
            </tables>
        </sub_module>
